[user]
  name = Jamie Barton
  email = hi_jamie@me.com
[shell]
  username = notrab
[github]
  username = notrab
[credential]
  helper = osxkeychain
[core]
  editor = atom --wait
  whitespace = trailing-space,cr-at-eol
[alias]
  co = checkout
  b = branch
  di = diff
  aa = add --all
  amend = comment --amend
  fa = fetch --all
  r = rebase
  rc = rebase --continue
  ra = rebase --abort
  rs = rebase --skip
[pull]
  rebase = true
[push]
  default = simple
